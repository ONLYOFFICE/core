{
  "manifest_version": 3,
  "name": "ONLYOFFICE Keychain",
  "version": "1.0",
  "permissions": ["tabs"],
  "icons": {
	"16": "icons/icon16.png",
	"32": "icons/icon32.png",
	"48": "icons/icon48.png",
	"64": "icons/icon64.png",
	"128": "icons/icon128.png"
  },
  "background": {
	"service_worker": "js/background.js"
  },
  "action": {
	"default_icon": {
	  "16": "icons/icon16.png",
	  "32": "icons/icon32.png",
	  "48": "icons/icon48.png",
	  "64": "icons/icon64.png",
	  "128": "icons/icon128.png"
	},
	"default_popup": "src/popup/index.html"
  },
  "content_scripts": [
	{
	  "matches": ["<all_urls>"],
	  "js": ["js/content.js"]
	}
  ],
  "web_accessible_resources": [
	{
	  "resources": ["*.wasm"],
	  "matches": ["<all_urls>"]
	}
  ]
}
